{
  "name": "brimmar/phpstan-rustlike-result-extension",
  "version": "1.0.0",
  "description": "PHPStan extension to help the Result Type implementation",
  "type": "phpstan-extension",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Brimmar\\PHPStan\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Brimmar\\PHPStan\\Tests\\": "tests/",
        "Brimmar\\PhpResult\\Interfaces\\": "tests/Rules/"
    }
  },
  "authors": [
    {
      "name": "Bryan",
      "email": "bryan.sergio@brimmar.dev"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^10.1"
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  }
}
